{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "nette/finder": "^2.4",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.13",
        "symplify/token-runner": "^4.8",
        "slam/php-cs-fixer-extensions": "^1.17",
        "thecodingmachine/safe": "^0.1.3"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "symplify/easy-coding-standard-tester": "^4.8",
        "symplify/package-builder": "^4.8",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.8-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
